library(ggplot2)
library(reshape2)
df <- read.csv("C:/Users/Lenovo/OneDrive/Desktop/3-1/Book1.csv", header = TRUE)
contingency_table <- table(df$Age, df$Marks)
print("Contingency Table:")
print(contingency_table)
chi_test <- chisq.test(contingency_table)
cat("\nChi-Square Statistic:", chi_test$statistic, "\n")
cat("P-Value:", chi_test$p.value, "\n")
cat("Degrees of Freedom:", chi_test$parameter, "\n")
cat("Expected Frequencies:\n")
print(chi_test$expected)
